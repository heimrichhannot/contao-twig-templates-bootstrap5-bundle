{
  "name": "heimrichhannot/contao-twig-templates-bootstrap5-bundle",
  "type": "contao-bundle",
  "description": "Brings bootstrap 5 template to twig templates bundle",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.2",
    "contao/core-bundle": "^4.4",
    "heimrichhannot/contao-twig-templates-bundle": "^2.0",
    "heimrichhannot/contao-utils-bundle": "^2.101"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\TwigTemplatesBootstrap5Bundle\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\TwigTemplatesBootstrap5Bundle\\ContaoManager\\Plugin",
    "foxy": true
  }
}
